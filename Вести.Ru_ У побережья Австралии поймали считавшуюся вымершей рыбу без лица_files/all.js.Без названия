                    _this.$layout.addClass('content_nav-mobile-close').removeClass('content_nav-mobile-open');

                    $('html, body, .page-wrap').css({'overflow': 'visible', 'height' : 'auto'});
                },

                allowPageScroll: 'auto',

                threshold: 75
            });



        }

    }).init();

